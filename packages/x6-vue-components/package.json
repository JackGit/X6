{
  "name": "@antv/x6-vue-components",
  "version": "1.0.1",
  "description": "Vue components for building x6 editors",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*"
  ],
  "scripts": {
    "clean": "rimraf dist es lib",
    "lint": "run-s lint:ts lint:vue",
    "lint:vue": "echo \"hi\"",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json --fix",
    "build:esm": "rollup --config rollup.config.js --format es --file es/index.js",
    "build:cjs": "rollup --config rollup.config.js --format cjs --file lib/index.js",
    "build": "run-p build:cjs build:esm",
    "prebuild": "run-s lint clean",
    "watch": "watch 'yarn build' ./src",
    "prepare": "yarn build",
    "precommit": "lint-staged"
  },
  "keywords": [
    "component",
    "vue",
    "x6",
    "x6-editor",
    "editor",
    "ui",
    "antv"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "tslint -c tslint.json -p ./tsconfig.json --fix"
    ],
    "*.js": [
      "eslint --fix"
    ],
    "src/**/*.less": [
      "stylelint --syntax less --fix"
    ]
  },
  "dependencies": {
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.4",
    "lint-staged": "^10.5.3",
    "rollup": "^2.34.2",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.1.2",
    "vue": "^2.6.11"
  },
  "author": {
    "name": "bubkoo",
    "email": "bubkoo.wy@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/antvis/x6",
  "bugs": {
    "url": "https://github.com/antvis/x6/issues"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/antvis/x6.git",
    "directory": "packages/x6-vue-components"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
